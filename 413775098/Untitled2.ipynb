    "n = int(input(\"Enter the number:\"))\n",
    "total = 0\n",
    "if n == 1:\n",
    "    print(\"n=1\")\n",
    "else:\n",
    "    for i in range(1, n+1):\n",
    "        total += i\n",
    "    print(f\"{1}+{2}+.....+{n} equal:\", total)"
